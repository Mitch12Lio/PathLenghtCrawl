<Window x:Class="NavigatorTemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigatorTemplate"
        mc:Ignorable="d"
        Title="Path Length Crawler 1.1 - Written by Mitch Girard" Height="500" Width="800">
    <DockPanel LastChildFill="True">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem Header="_Open" />
                <MenuItem Header="_Close" />
                <MenuItem Header="_Save" />
            </MenuItem>
            <MenuItem Header="_Edit">
                <MenuItem Header="_Cut" />
                <MenuItem Header="_Copy" />
                <MenuItem Header="_Paste" />
            </MenuItem>
        </Menu>
        <StatusBar DockPanel.Dock="Bottom">
            <StatusBarItem>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" Content="Status:"></Label>
                    <Label Grid.Row="0" Grid.Column="1" Content="{Binding Master.StatusMessage}"></Label>
                </Grid>
            </StatusBarItem>
            <StatusBarItem HorizontalAlignment="Right">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition Width="40" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="1" Content="Running Time:"></Label>
                    <Label Grid.Row="0" Grid.Column="2" Content="{Binding Master.ValidatorRunningTime}"></Label>
                    <Button Grid.Row="0" Grid.Column="3" Content="Start" Command="{Binding Master.StartTimerCommand}"></Button>
                    <Button Grid.Row="0" Grid.Column="4" Content="Stop" Command="{Binding Master.StopTimerCommand}"></Button>
                    <Rectangle Grid.Row="0" Grid.Column="0" VerticalAlignment="Stretch" Width="1" Margin="2" Stroke="Black" />
                </Grid>
            </StatusBarItem>
        </StatusBar>
        <TabControl>
            <TabItem Header="Settings">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />

                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="0" Padding="3">
                        <GroupBox.Header>
                            <Span FontWeight="DemiBold">Behaviour</Span>
                        </GroupBox.Header>
                        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />

                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <CheckBox Grid.Row="0" Grid.Column="0" Content="Include Folders" IsChecked="{Binding Master.IncludeFolders}"></CheckBox>
                            <CheckBox Grid.Row="0" Grid.Column="1" Content="Include Sub-Folders" IsChecked="{Binding Master.IncludeSubFolders}" IsEnabled="False"></CheckBox>

                            <CheckBox Grid.Row="1" Grid.Column="0" Content="Include Documents" IsChecked="{Binding Master.IncludeDocuments}"></CheckBox>
                            <CheckBox Grid.Row="1" Grid.Column="1" Content="Include Empty Folders" IsChecked="{Binding Master.IncludeEmptyFolders}" IsEnabled="False"></CheckBox>

                            <CheckBox Grid.Row="2" Grid.Column="1" Content="AutoSave Report" IsChecked="True" IsEnabled="False"></CheckBox>
                        </Grid>
                    </GroupBox>
                    <GroupBox BorderThickness="3" Grid.Column="1" Grid.Row="0" Padding="3">
                        <GroupBox.Header>
                            <Span FontWeight="DemiBold">Preference</Span>
                        </GroupBox.Header>
                        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0" Grid.Column="1" Content="Min Path Length:"></Label>
                            <TextBox Grid.Row="0" Grid.Column="2" Text="{Binding Master.MinPathLength}"></TextBox>
                            <Label Grid.Row="1" Grid.Column="1" Content="Max Path Length:"></Label>
                            <TextBox Grid.Row="1" Grid.Column="2" Text="{Binding Master.MaxPathLength}" IsEnabled="False"></TextBox>

                            <RadioButton Grid.Row="0" Grid.Column="0" VerticalAlignment="Bottom" GroupName="SpeedDetailsRB" Content="Speed" IsChecked="{Binding Master.Speed}"></RadioButton>
                            <RadioButton Grid.Row="1" Grid.Column="0" GroupName="SpeedDetailsRB" Content="Details" IsChecked="{Binding Master.Details}"></RadioButton>
                        </Grid>
                    </GroupBox>
                    <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2">
                        <GroupBox.Header>
                            <Span FontWeight="DemiBold">Logs</Span>
                        </GroupBox.Header>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <CheckBox Grid.Row="0" Grid.Column="0"  Content="Create Logs" IsChecked="True" IsEnabled="False"></CheckBox>
                            <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Content="Location"></Label>
                            <TextBox Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Text="{Binding Master.LogLocationTxt}"></TextBox>
                            <Button Grid.Row="3" Grid.Column="0" Content="Folder..." CommandParameter="LogLocationTxt" Command="{Binding Master.SetFolderCommand}"></Button>
                            <Button Grid.Row="3" Grid.Column="1" Content="Folder (via TreeView)..." CommandParameter="LogLocationTxt" Command="{Binding Master.SetFolderViaTVCommand}" IsEnabled="False"></Button>
                            <!--<Separator Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2"></Separator>
                            <Label Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" Content="Audit Database"></Label>
                            <TextBox Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="2" Text="{Binding Master.FilePath}"></TextBox>
                            <Button Grid.Row="7" Grid.Column="0" Content="File..." CommandParameter="FilePathTxt" Command="{Binding Master.SetFileCommand}"></Button>
                            <Button Grid.Row="7" Grid.Column="1" Content="File (via TreeView)..." CommandParameter="FilePathTxt" Command="{Binding Master.SetFileViaTVCommand}" IsEnabled="False"></Button>-->
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="Bulk">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <GroupBox BorderThickness="3" Grid.Column="2" Grid.Row="0" Padding="3" Visibility="{Binding Master.ExpandOptionsStats}">
                        <GroupBox.Header>
                            <Span FontWeight="DemiBold">Statistics</Span>
                        </GroupBox.Header>
                        <Grid Grid.Row="0" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Text="Per" FontSize="10" HorizontalAlignment="Left" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.ValidatorRunningTimePer}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="2" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="3" Grid.Column="0" Text="Paths Imported:" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.PathImportedCount}"></Run>
                                <Run Text="/"></Run>
                                <Run Text="{Binding Master.PathImportedCountTotal}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="5" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="6" Grid.Column="0" Text="Paths Processed:" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="7" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.PathProcessedCount}"></Run>
                                <Run Text="/"></Run>
                                <Run Text="{Binding Master.PathProcessedCountTotal}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="8" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="9" Grid.Column="0" Text="LP Document Count" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="10" Grid.Column="0" Text="(Path/Batch)" HorizontalAlignment="Center" FontSize="8" FontFamily="Times New Roman"></TextBlock>
                            <TextBlock Grid.Row="11" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.FileCountTotal}"></Run>
                                <Run Text="/"></Run>
                                <Run Text="{Binding Master.FileCount}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="12" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="13" Grid.Column="0" Text="LP Folder Count" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="14" Grid.Column="0" Text="(Path/Batch)" HorizontalAlignment="Center" FontSize="8" FontFamily="Times New Roman"></TextBlock>
                            <TextBlock Grid.Row="15" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.FolderCountTotal}"></Run>
                                <Run Text="/"></Run>
                                <Run Text="{Binding Master.FolderCount}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="16" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="17" Grid.Column="0" Text="LP Total Count" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="18" Grid.Column="0" Text="(Path/Batch)" HorizontalAlignment="Center" FontSize="8" FontFamily="Times New Roman"></TextBlock>
                            <TextBlock Grid.Row="19" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.ObjectCountTotal}"></Run>
                                <Run Text="/"></Run>
                                <Run Text="{Binding Master.ObjectCount}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="20" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="21" Grid.Column="0" Text="Warning Count" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="22" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.WarningCount}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="23" Grid.Column="0"></Separator>

                            <TextBlock Grid.Row="24" Grid.Column="0" Text="Error Count" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Bottom"></TextBlock>
                            <TextBlock Grid.Row="25" Grid.Column="0" HorizontalAlignment="Center" FontSize="10" FontWeight="Heavy">
                                <Run Text="{Binding Master.ErrorCount}"></Run>
                            </TextBlock>

                            <Separator Grid.Row="26" Grid.Column="0"></Separator>

                        </Grid>
                    </GroupBox>
                    <GridSplitter Grid.Column="1" Grid.Row="0" Width="3" HorizontalAlignment="Stretch" />
                    <Grid Grid.Row="0" Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <CheckBox Grid.Row="0" Grid.Column="0" Content="Expanded Components" IsChecked="{Binding Master.ExpandOptionsStatsBool}"></CheckBox>
                        <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" Visibility="{Binding Master.ExpandOptionsStats}">
                            <GroupBox.Header>
                                <Span FontWeight="DemiBold">Options</Span>
                            </GroupBox.Header>
                            <StackPanel>
                                <!--<StackPanel Orientation="Horizontal">
                                    <RadioButton Content="Read File" GroupName="FileOrDrive" Margin="10,0,10,0"></RadioButton>
                                    <RadioButton Content="Read Drive" GroupName="FileOrDrive"></RadioButton>
                                </StackPanel>-->
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Row="0" Grid.Column="0" Text="{Binding Master.PathFileImportTxt}"></TextBox>
                                        <CheckBox Margin="3,0,3,0" Grid.Row="0" Grid.Column="1" Content="Omit NDS://HEALTH_TREE prefix" IsChecked="True" IsEnabled="False"></CheckBox>
                                    </Grid>
                                    <Grid Grid.Row="1" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Button Grid.Row="0" Grid.Column="0" Content="Select..." CommandParameter="PathFileImportTxt" Command="{Binding Master.SetFileCommand}" IsEnabled="{Binding Master.SelectBulkFile2ProcessButtonEnabled}"></Button>
                                        <Button Grid.Row="0" Grid.Column="1" Content="Import &amp; Process" Command="{Binding Master.WhateverStartLongWayCommand}" IsEnabled="{Binding Master.ImportProcessBulkFileButtonEnabled}"></Button>
                                    </Grid>
                                </Grid>
                            </StackPanel>
                        </GroupBox>
                        <GridSplitter Grid.Column="0" Grid.Row="2" Grid.ColumnSpan ="2" Height="3" HorizontalAlignment="Stretch"></GridSplitter>
                        <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="2">
                            <GroupBox.Header>
                                <Span FontWeight="DemiBold">Files</Span>
                            </GroupBox.Header>
                            <ListView ItemsSource="{Binding Master.UNCObjectFileLst}" VerticalContentAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True">
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="ListViewItem">
                                        <Setter Property="HorizontalContentAlignment" Value="Stretch"></Setter>
                                        <Setter Property="VerticalContentAlignment" Value="Stretch"></Setter>
                                    </Style>
                                </ListView.ItemContainerStyle>
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Header="Count" Width="50">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding Count}" HorizontalAlignment="Center" Foreground="{Binding HomeNameExistForeground}" FontWeight="{Binding HomeNameExistFontWeight}"></TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="Length" Width="50">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding CharacterCount}" HorizontalAlignment="Center" Foreground="{Binding HomeNameExistForeground}" FontWeight="{Binding HomeNameExistFontWeight}"></TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="UNC Path" Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox ToolTip="{Binding NameUNC}" Text="{Binding NameUNC}" IsReadOnly ="True" HorizontalAlignment="Left" Foreground="{Binding MemberExistForeground}" FontWeight="{Binding MemberExistFontWeight}"></TextBox>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </GroupBox>
                        <GridSplitter Grid.Column="0" Grid.Row="4" Grid.ColumnSpan ="2" Height="3" HorizontalAlignment="Stretch"></GridSplitter>
                        <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="2">
                            <GroupBox.Header>
                                <Span FontWeight="DemiBold">Folders</Span>
                            </GroupBox.Header>
                            <ListView Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2" ItemsSource="{Binding Master.UNCObjectFolderLst}" VerticalContentAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True">
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="ListViewItem">
                                        <Setter Property="HorizontalContentAlignment" Value="Stretch"></Setter>
                                        <Setter Property="VerticalContentAlignment" Value="Stretch"></Setter>
                                    </Style>
                                </ListView.ItemContainerStyle>
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Header="Count" Width="50">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding Count}" HorizontalAlignment="Center" Foreground="{Binding HomeNameExistForeground}" FontWeight="{Binding HomeNameExistFontWeight}"></TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="Length" Width="50">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding CharacterCount}" HorizontalAlignment="Center" Foreground="{Binding HomeNameExistForeground}" FontWeight="{Binding HomeNameExistFontWeight}"></TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="UNC Path" Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox ToolTip="{Binding NameUNC}" Text="{Binding NameUNC}" IsReadOnly ="True" HorizontalAlignment="Left" Foreground="{Binding MemberExistForeground}" FontWeight="{Binding MemberExistFontWeight}"></TextBox>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </GroupBox>
                        <GridSplitter Grid.Column="0" Grid.Row="6" Grid.ColumnSpan ="2" Height="3" HorizontalAlignment="Stretch"></GridSplitter>
                        <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="7" Grid.ColumnSpan="2" Visibility="{Binding Master.ExpandOptionsStats}">
                            <GroupBox.Header>
                                <Span FontWeight="DemiBold">Status</Span>
                            </GroupBox.Header>


                            <Grid Grid.Row="0" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>

                                <Label Grid.Row="0" Grid.Column="0" Content="Processing:" FontSize="10"></Label>
                                <TextBox Grid.Row="0" Grid.Column="1" IsReadOnly ="True" Text="{Binding Master.PathCurrentlyProcessing}"></TextBox>
                                <Label Grid.Row="1" Grid.Column="0" Content="Counting:" FontSize="10"></Label>
                                <TextBox Grid.Row="1" Grid.Column="1" IsReadOnly ="True" Text="{Binding Master.PathCurrentlyCounting}"></TextBox>
                            </Grid>

                        </GroupBox>
                        <Grid Grid.Row="8" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="50"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Button Grid.Row="0" Grid.Column="1" Content="Skip" Command="{Binding Master.SkipPathCommand}"></Button>
                            <Button Grid.Row="0" Grid.Column="2" Content="Pause" Command="{Binding Master.PauseCommand}" IsEnabled="False"></Button>
                            <Button Grid.Row="0" Grid.Column="3" Content="Quit" Command="{Binding Master.QuitLoopCommand}"></Button>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Other">
                <TabControl>

                    <TabItem Header="NDS Process">
                        <DockPanel LastChildFill="True">
                            <Grid DockPanel.Dock="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Label Grid.Row="0" Grid.Column="0" Content="Read:"></Label>
                                <Label Grid.Row="0" Grid.Column="1" Content="{Binding Master.CSVReadCount}"></Label>
                                <Label Grid.Row="0" Grid.Column="2" Content="Write:"></Label>
                                <Label Grid.Row="0" Grid.Column="3" Content="{Binding Master.CSVWriteCount}"></Label>
                            </Grid>
                            <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" Visibility="{Binding Master.ExpandOptionsStats}">
                                <GroupBox.Header>
                                    <Span FontWeight="DemiBold">NDS Log</Span>
                                </GroupBox.Header>
                                <StackPanel>
                                    <GroupBox>
                                        <StackPanel>
                                            <RadioButton Content="Files" GroupName="NDSLogType" IsChecked="{Binding Master.NDSLogFileType}"></RadioButton>
                                            <RadioButton Content="Folders" GroupName="NDSLogType" IsChecked="{Binding Master.NDSLogFolderType}"></RadioButton>
                                        </StackPanel>
                                    </GroupBox>

                                    <Label Content="Log to process:"></Label>
                                    <TextBox Text="{Binding Master.NDSLog2Process}"></TextBox>
                                    <Button Content="Select..." CommandParameter="NDSLog2Process" Command="{Binding Master.SetFileCommand}"></Button>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Label Grid.Row="0" Grid.Column="0" Content="Destination"></Label>
                                        <CheckBox Grid.Row="0" Grid.Column="1" Content="Same path as above" IsChecked="{Binding Master.NDSSamePathAsAbove}"></CheckBox>
                                    </Grid>
                                    <TextBox Text="{Binding Master.NDSLogDestination}"></TextBox>
                                    <Button Content="Save To..." CommandParameter="NDSLogDestination" Command="{Binding Master.SetFolderCommand}"></Button>
                                    <Separator></Separator>
                                    <Button Content="Process" Command="{Binding Master.ProcessNDSLogCommand}"></Button>

                                </StackPanel>
                            </GroupBox>
                        </DockPanel>
                    </TabItem>
                    <TabItem Header="Star">
                        <DockPanel LastChildFill="True">
                            <Grid DockPanel.Dock="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Label Grid.Row="0" Grid.Column="0" Content="Read:"></Label>
                                <Label Grid.Row="0" Grid.Column="1" Content="{Binding Master.CSVReadCount}"></Label>
                                <Label Grid.Row="0" Grid.Column="2" Content="Write:"></Label>
                                <Label Grid.Row="0" Grid.Column="3" Content="{Binding Master.CSVWriteCount}"></Label>
                            </Grid>
                            <GroupBox BorderThickness="3" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" Visibility="{Binding Master.ExpandOptionsStats}">
                                <GroupBox.Header>
                                    <Span FontWeight="DemiBold">File</Span>
                                </GroupBox.Header>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
